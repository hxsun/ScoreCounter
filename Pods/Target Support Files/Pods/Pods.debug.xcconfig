GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/MagicalRecord" "${PODS_ROOT}/Headers/Public/PXAlertView" "${PODS_ROOT}/Headers/Public/XLForm"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/MagicalRecord" -isystem "${PODS_ROOT}/Headers/Public/PXAlertView" -isystem "${PODS_ROOT}/Headers/Public/XLForm"
OTHER_LDFLAGS = -ObjC -l"Pods-MagicalRecord" -l"Pods-PXAlertView" -l"Pods-XLForm" -framework "CoreData" -framework "CoreGraphics" -framework "Foundation" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods